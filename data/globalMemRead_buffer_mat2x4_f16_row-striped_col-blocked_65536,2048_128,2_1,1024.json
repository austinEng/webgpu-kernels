{
  "shader": "globalMemRead_30.wgsl",
  "requiredFeatures": [
    "timestamp-query",
    "shader-f16"
  ],
  "requiredLimits": {
    "maxStorageBufferBindingSize": 268435456,
    "maxBufferSize": 268435456
  },
  "params": {
    "storage_type": "buffer",
    "data_type": "f16",
    "pack_type": "mat2x4",
    "row_access": "striped",
    "col_access": "blocked",
    "size": [
      65536,
      2048
    ],
    "workgroup_size": [
      128,
      2
    ],
    "dispatch_size": [
      1,
      1024
    ]
  }
}